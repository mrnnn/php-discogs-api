{
    "name": "ricbra/php-discogs-api",
    "description": "The Discogs API makes it easy for developers to communicate with the Discogs platform",
    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/guzzle-services": "^1.1",
        "guzzlehttp/oauth-subscriber": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "Iain Mullan",
            "email": "iain.mullan@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Discogs": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "ricbra/discogs-bundle": "For integrating Discogs into Symfony2"
    }
}
